{
  "name": "s-tier-template",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "dev:website": "turbo run dev --filter=website",
    "dev:sponsor": "turbo run dev --filter=sponsor",
    "check-types": "turbo run check-types",
    "clean": "turbo clean",
    "update": "pnpm update -r",
    "check": "ultracite check",
    "format": "biome format --write",
    "fix": "ultracite fix",
    "prepare": "lefthook install",
    "find-mismatches": "pnpm dlx syncpack list-mismatches"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.0",
    "lefthook": "^2.1.1",
    "turbo": "^2.8.10",
    "typescript": "catalog:",
    "ultracite": "7.2.3"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
